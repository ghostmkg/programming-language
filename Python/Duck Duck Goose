import random
import pygame
pygame.init()

select_list = ["duck", "duck", "goose", "duck", "duck", "duck"]
 
pick = "duck"
 
while pick != "goose":

    index = random.randint(0, len(select_list) -1)

    pick = select_list[index]
 
    pygame.time.wait(1500)
 
 
    if pick == "duck":
 
        print("Duck...")
 
    else:
 
        print("Goose!")
